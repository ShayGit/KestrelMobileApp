<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/kestrelfan"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintHorizontal_bias="0.58"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.09"
        app:layout_constraintWidth_percent="0.55"
        app:srcCompat="@drawable/kestrelfan" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/kestrel"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kestrelback" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/kestrelLight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/kestrel_light"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.135"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.49"
        app:layout_constraintWidth_percent="0.79" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/measurementIcon1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.54"
        app:layout_constraintWidth_percent="0.04" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/measurementIcon2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:layout_marginStart="3dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toStartOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/measurementIcon1"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.54"
        app:layout_constraintWidth_percent="0.04" />


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/measurementIcon3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="3dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/measurementIcon2"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.54"
        app:layout_constraintWidth_percent="0.04" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/measurementIcon4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="3dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/measurementIcon3"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.54"
        app:layout_constraintWidth_percent="0.04" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/powerButton"
        style="@style/AppTheme.KestrelButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/leftButton"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.688"
        app:layout_constraintWidth_percent="0.24" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/rightButton"
        style="@style/AppTheme.KestrelButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/powerButton"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.688"
        app:layout_constraintWidth_percent="0.37" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/leftButton"
        style="@style/AppTheme.KestrelButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.688"
        app:layout_constraintWidth_percent="0.37" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/measurementTextView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fitsSystemWindows="false"
        android:gravity="top|end"
        android:lines="1"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@+id/kestrel"
        app:layout_constraintHeight_percent="0.105"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="@+id/kestrel"
        app:layout_constraintTop_toTopOf="@+id/kestrel"
        app:layout_constraintVertical_bias="0.475"
        app:layout_constraintWidth_percent="0.65" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/measurementIconText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="3dp"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/measurementIcon4"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.539"
        app:layout_constraintWidth_percent="0.08" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/holdText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHeight_percent="0.03"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"
        app:layout_constraintVertical_bias="0.539"
        app:layout_constraintWidth_percent="0.15" />

    <ProgressBar
        android:id="@+id/pb"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.49"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintWidth_percent="0.3"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>

    <View
        android:id="@+id/temperature_thermistor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintVertical_bias="0.33"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintWidth_percent="0.2"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>


    <View
        android:id="@+id/pressure_sensor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintVertical_bias="0.3"
        app:layout_constraintHeight_percent="0.065"
        app:layout_constraintWidth_percent="0.19"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>
    <View
        android:id="@+id/serial_number"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintVertical_bias="0.36"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintWidth_percent="0.3"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>
    <View
        android:id="@+id/humidity_sensor"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.41"
        app:layout_constraintVertical_bias="0.353"
        app:layout_constraintHeight_percent="0.063"
        app:layout_constraintWidth_percent="0.18"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>

    <View
        android:id="@+id/battery_door"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/kestrel"
        app:layout_constraintEnd_toEndOf="@id/kestrel"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintVertical_bias="0.96"
        app:layout_constraintHeight_percent="0.3"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintStart_toStartOf="@id/kestrel"
        app:layout_constraintTop_toTopOf="@id/kestrel"/>
</androidx.constraintlayout.widget.ConstraintLayout>
